[run]
source = src

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise NotImplementedError
    if __name__ == .__main__.:
    pass
    raise ImportError
omit =
    # ベースクラスやインターフェース以外のdetector実装は除外
    src/detectors/*.py

include =
    src/detectors/base_detector.py
    src/detectors/__init__.py

[paths]
source =
    src/
    */site-packages/

[html]
directory = coverage_html_report

[xml]
output = coverage.xml 